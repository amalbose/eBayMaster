ITEMS=CREATE TABLE <DATABASE_NAME>.ITEMS (ITEMID INTEGER NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1), ITEMNAME VARCHAR(50) NOT NULL, CATEGORY VARCHAR(50),	RATE FLOAT, PRIMARY KEY (ITEMID))

TRANSACTIONS=CREATE TABLE <DATABASE_NAME>.TRANSACTIONS (TRANSACTIONID INTEGER NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1), ITEMNAME VARCHAR(50), CATEGORYID INTEGER NOT NULL, BUYERNAME VARCHAR(50), LOCATION VARCHAR(50), COST FLOAT, PRICE FLOAT, PROFIT FLOAT, DATE TIMESTAMP, PRIMARY KEY (TRANSACTIONID))


