# Query to get details from Transactions table
QUERY_TRANS_TABLE=SELECT TRANSACTIONID, ITEMID, BUYERNAME, LOCATION, COST, PRICE, PROFIT, DATE FROM <DATABASE_NAME>.TRANSACTIONS

# Query to get details from Items table
QUERY_ITEMS_TABLE=SELECT ITEMID, ITEMNAME, CATEGORY, RATE FROM <DATABASE_NAME>.ITEMS

# Query to get full details from Transaction and Items table
# IMPORTANT - Modify TransactionsTableModel->getTable method on modifying the column order.
QUERY_TRANS_DETAIL=SELECT T.TRANSACTIONID, T.ITEMID, I.ITEMNAME, I.CATEGORY, I.RATE, T.BUYERNAME, T.LOCATION, T.COST, T.PRICE, T.PROFIT, T.DATE FROM <DATABASE_NAME>.TRANSACTIONS AS T, <DATABASE_NAME>.ITEMS AS I WHERE I.ITEMID = T.ITEMID ORDER BY T.TRANSACTIONID DESC

# Get Transaction Item details from Item Name
QUERY_ITEM_WITH_NAME=SELECT ITEMID, ITEMNAME, CATEGORY, RATE FROM <DATABASE_NAME>.ITEMS WHERE ITEMNAME=? 

#Get all distinct items
QUERY_ALL_ITEMS=SELECT DISTINCT ITEMNAME FROM <DATABASE_NAME>.ITEMS

#Get all distinct categories
QUERY_ALL_CATEGORIES=SELECT DISTINCT CATEGORY FROM <DATABASE_NAME>.ITEMS