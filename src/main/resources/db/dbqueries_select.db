# Query to get details from Transactions table
QUERY_TRANS_TABLE=SELECT TRANSACTIONID, CATEGORYID, ITEMNAME, BUYERNAME, LOCATION, COST, PRICE, PROFIT, DATE FROM <DATABASE_NAME>.TRANSACTIONS

# Query to get details from Categories table
QUERY_CATEGORY_TABLE=SELECT CATEGORYID, CATEGORYNAME, RATE FROM <DATABASE_NAME>.CATEGORIES ORDER BY CATEGORYID DESC

# Query to get full details from Transaction and Items table
# IMPORTANT - Modify TransactionsTableModel->getTable method on modifying the column order.
QUERY_TRANS_DETAIL=SELECT T.TRANSACTIONID, T.CATEGORYID, T.ITEMNAME, I.CATEGORYNAME, I.RATE, T.BUYERNAME, T.LOCATION, T.COST, T.PRICE, T.PROFIT, T.DATE FROM <DATABASE_NAME>.TRANSACTIONS AS T, <DATABASE_NAME>.CATEGORIES AS I WHERE I.CATEGORYID = T.CATEGORYID ORDER BY T.TRANSACTIONID DESC

# Get Category details from Category Name and Rate
QUERY_CATEGORY_WITH_DETAIL=SELECT CATEGORYID, CATEGORYNAME, RATE FROM <DATABASE_NAME>.CATEGORIES WHERE CATEGORYNAME=? AND RATE=?

#Get all distinct items
QUERY_ALL_ITEMS=SELECT DISTINCT ITEMNAME FROM <DATABASE_NAME>.TRANSACTIONS ORDER BY ITEMNAME

#Get all distinct categories
QUERY_ALL_CATEGORIES=SELECT DISTINCT CATEGORYNAME FROM <DATABASE_NAME>.CATEGORIES ORDER BY CATEGORYNAME

# Get category name from item
QUERY_CATEGORY_FROM_ITEM=SELECT I.CATEGORYNAME FROM <DATABASE_NAME>.TRANSACTIONS AS T, <DATABASE_NAME>.CATEGORIES AS I WHERE I.CATEGORYID = T.CATEGORYID AND T.ITEMNAME LIKE ? ORDER BY T.TRANSACTIONID DESC