# Query to get details from Transactions table
QUERY_TRANS_TABLE=SELECT TRANSACTIONID, ITEMID, BUYERNAME, LOCATION, COST, PRICE, PROFIT, DATE FROM EBAYMASTERDB.TRANSACTIONS

# Query to get details from Items table
QUERY_ITEMS_TABLE=SELECT ITEMID, ITEMNAME, CATEGORY, RATE FROM EBAYMASTERDB.ITEMS;

# Query to get full details from Transaction and Items table
QUERY_TRANS_DETAIL=SELECT T.TRANSACTIONID, T.ITEMID, T.BUYERNAME, T.LOCATION, T.COST, T.PRICE, T.PROFIT, T.DATE, I.ITEMNAME, I.CATEGORY, I.RATE FROM EBAYMASTERDB.TRANSACTIONS AS T, EBAYMASTERDB.ITEMS AS I WHERE I.ITEMID = T.ITEMID