#Query to insert data into Transactions table
INSERT_TRANS_TABLE=INSERT INTO <DATABASE_NAME>.TRANSACTIONS(ITEMID, BUYERNAME, LOCATION, COST, PRICE, PROFIT, DATE) VALUES (?,?,?,?,?,?,?)

#Query to insert data to Items table
INSERT_ITEMS_TABLE=INSERT INTO <DATABASE_NAME>.ITEMS(ITEMNAME, CATEGORY, RATE) VALUES (?,?,?)